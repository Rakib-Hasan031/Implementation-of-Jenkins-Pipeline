pipeline {
  agent {
    docker {image 'node:16-alpine'}
  }
  options {
    skipDefaultCheckout(true)
    parallelsAlwaysFailFast()
    disableConcurrentBuilds()
    buildDiscarder(logRotator(numToKeepStr: ‘20’, artifactNumToKeepStr: ‘20’))
  }
stages {
  stage ('Test') {
    steps {
      sh 'node --version'
    }
  }
}
}
